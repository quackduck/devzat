{
  "name": "devzat",
  "version": "0.1.0",
  "license": "MIT",
  "author": "merlin04",
  "main": "dist/index.js",
  "module": "dist/devzat.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate-grpc": "grpc_tools_node_protoc --js_out=import_style=es,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ../../plugin ../../plugin/plugin.proto && protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/generated -I ../../plugin ../../plugin/plugin.proto",
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "copy-dts": "mkdir dist/generated && cp src/generated/*.d.ts dist/generated/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/devzat.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/devzat.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/google-protobuf": "^3.15.5",
    "dts-cli": "^1.4.0",
    "grpc-tools": "^1.11.2",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "husky": "^7.0.4",
    "rollup-plugin-binary2base64": "^1.0.3",
    "size-limit": "^7.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.2"
  }
}
